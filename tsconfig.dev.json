{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "module": "commonjs",
    "target": "es2024",
    "types": ["node"],
    "paths": {
      "": ["./src/utils/types/"]
    },
    "removeComments": true,
    "noImplicitReturns": true,
    "noImplicitOverride": true,
    "noUnusedLocals": true,
    "esModuleInterop": true
  }
}